{
  "url": "https://novelbuddy.io${0}",
  "type": "GET",
  "bodyType": "JSON",
  "waitForPageLoad": false,
  "query": [
    {
      "label": "novel_title",
      "element": ".chapter-info>h2>a",
      "getContent": true,
      "withHref": false
    },
    {
      "label": "novel_url",
      "element": ".chapter-info>h2>a",
      "getContent": true,
      "withHref": true
    },
    {
      "label": "title",
      "element": "#chapter__content>h1",
      "getContent": true,
      "withHref": false
    },
    {
      "label": "content",
      "element": ".content-inner>p",
      "getContent": true,
      "withHref": false
    },
    {
      "label": "PreviousPage",
      "element": "#btn-prev",
      "getContent": true,
      "withHref": true
    },
    {
      "label": "NextPage",
      "element": "#btn-next",
      "getContent": true,
      "withHref": true
    }
  ]
}