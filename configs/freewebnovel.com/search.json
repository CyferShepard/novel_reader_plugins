{
  "url": "https://freewebnovel.com/search",
  "type": "POST",
  "body": "searchkey=searchterm",
  "bodyType": "FORM_DATA",
  "query": [
    {
      "label": "results",
      "element": ".con",
      "getContent": true,
      "withHref": false,
      "subQuery": [
        {
          "label": "url",
          "element": ".tit>a",
          "getContent": true,
          "withHref": true,
          "selectItemsAtIndex": []
        },
        {
          "label": "title",
          "element": ".tit>a",
          "getContent": true,
          "withHref": false,
          "dataProp": "title",
          "selectItemsAtIndex": []
        },
        {
          "label": "summary",
          "getContent": true,
          "withHref": false,
          "selectItemsAtIndex": []
        },
        {
          "label": "cover",
          "element": ".pic>a>img",
          "getContent": true,
          "withHref": true,
          "selectItemsAtIndex": []
        },
        {
          "label": "genres",
          "element": ".txt>.desc>.item>.right>a",
          "getContent": true,
          "withHref": false,
          "selectItemsAtIndex": [
            0,
            1
          ]
        }
      ],
      "selectItemsAtIndex": []
    }
  ]
}