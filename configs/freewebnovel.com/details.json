{
  "url": "https://freewebnovel.com${0}",
  "type": "GET",
  "bodyType": "JSON",
  "waitForPageLoad": false,
  "waitDuration": 5000,
  "query": [
    {
      "label": "url",
      "element": ".cur.cur-1>.wp>a",
      "getContent": true,
      "withHref": true,
      "selectItemsAtIndex": [
        2
      ]
    },
    {
      "label": "cover",
      "element": ".m-imgtxt>.pic>img",
      "getContent": true,
      "withHref": true,
      "transformProcess": "(value)=>{\n        if (value.startsWith(\"/files\")) {\n          return \"https://freewebnovel.com\" + value;\n        } else {\n          return value;\n        }\n      }"
    },
    {
      "label": "title",
      "element": ".cur.cur-1>.wp>a",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": [
        2
      ]
    },
    {
      "label": "summary",
      "element": ".m-desc>.txt>.inner",
      "getContent": true,
      "withHref": false
    },
    {
      "label": "tags",
      "getContent": true,
      "withHref": false
    },
    {
      "label": "author",
      "element": ".m-imgtxt>.txt>.item>.right",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": [
        1
      ]
    },
    {
      "label": "status",
      "element": ".m-imgtxt>.txt>.item>.right>.s1.s2",
      "getContent": true,
      "withHref": false
    },
    {
      "label": "genres",
      "element": ".m-imgtxt>.txt>.item>.right",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": [
        2
      ],
      "transformProcess": "(value)=>value.split(\",\")"
    },
    {
      "label": "chapters",
      "getContent": true,
      "withHref": false
    },
    {
      "label": "lastUpdate",
      "getContent": true,
      "withHref": false
    }
  ]
}