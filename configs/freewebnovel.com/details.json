{
  "url": "https://freewebnovel.com${0}",
  "type": "GET",
  "body": "",
  "bodyType": "JSON",
  "query": [
    {
      "label": "url",
      "element": ".cur.cur-1>.wp>a",
      "getContent": true,
      "withHref": true,
      "selectItemsAtIndex": [
        2
      ]
    },
    {
      "label": "cover",
      "element": ".m-imgtxt>.pic>img",
      "getContent": true,
      "withHref": true,
      "selectItemsAtIndex": []
    },
    {
      "label": "title",
      "element": ".cur.cur-1>.wp>a",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": [
        2
      ]
    },
    {
      "label": "summary",
      "element": ".m-desc>.txt>.inner",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": []
    },
    {
      "label": "tags",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": []
    },
    {
      "label": "Author",
      "element": ".m-imgtxt>.txt>.item>.right",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": [
        1
      ]
    },
    {
      "label": "Status",
      "element": ".m-imgtxt>.txt>.item>.right>.s1.s2",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": []
    },
    {
      "label": "Genres",
      "element": ".m-imgtxt>.txt>.item>.right",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": [
        2
      ],
      "transformProcess": "(value)=>value.split(\",\")"
    },
    {
      "label": "Chapters",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": []
    },
    {
      "label": "LastUpdate",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": []
    }
  ]
}