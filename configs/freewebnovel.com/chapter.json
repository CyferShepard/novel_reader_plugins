{
  "url": "https://freewebnovel.com${0}",
  "type": "GET",
  "bodyType": "JSON",
  "waitForPageLoad": false,
  "query": [
    {
      "label": "novel_title",
      "element": ".tit>a",
      "getContent": true,
      "withHref": false,
      "dataProp": "title"
    },
    {
      "label": "novel_url",
      "element": ".tit>a",
      "getContent": true,
      "withHref": true
    },
    {
      "label": "title",
      "element": ".chapter",
      "getContent": true,
      "withHref": false
    },
    {
      "label": "content",
      "element": "#article",
      "getContent": true,
      "withHref": false
    },
    {
      "label": "PreviousPage",
      "element": "#prev_url",
      "getContent": true,
      "withHref": true,
      "transformProcess": "(value)=>value.split(\"/\").length - 1 > 2 ? value : \"\""
    },
    {
      "label": "NextPage",
      "element": "#next_url",
      "getContent": true,
      "withHref": true,
      "transformProcess": "(value)=>value.split(\"/\").length - 1 > 2 ? value : \"\""
    }
  ]
}