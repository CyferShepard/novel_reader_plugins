{
  "url": "https://novgo.net/search?keyword=${0}",
  "type": "GET",
  "bodyType": "JSON",
  "waitForPageLoad": false,
  "query": [
    {
      "label": "results",
      "element": ".row",
      "getContent": true,
      "withHref": false,
      "dataProp": "",
      "subQuery": [
        {
          "label": "url",
          "element": ".truyen-title>a",
          "getContent": true,
          "withHref": true
        },
        {
          "label": "title",
          "element": ".truyen-title>a",
          "getContent": true,
          "withHref": false
        },
        {
          "label": "summary",
          "getContent": true,
          "withHref": false
        },
        {
          "label": "cover",
          "element": "img",
          "getContent": true,
          "withHref": true,
          "transformProcess": "(value)=>{\n            if (value.startsWith(\"/\")) {\n              return \"https://novgo.net\" + value;\n            } else {\n              return value;\n            }\n          }"
        },
        {
          "label": "genres",
          "getContent": true,
          "withHref": false
        }
      ]
    }
  ]
}