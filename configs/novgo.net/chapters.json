{
  "url": "https://novgo.net${0}?page=${1}",
  "type": "GET",
  "bodyType": "JSON",
  "waitForPageLoad": false,
  "query": [
    {
      "label": "chapters",
      "element": "ul.list-chapter>li",
      "getContent": true,
      "withHref": false,
      "subQuery": [
        {
          "label": "url",
          "element": "a",
          "getContent": true,
          "withHref": true
        },
        {
          "label": "index",
          "element": "a",
          "getContent": true,
          "withHref": false,
          "dataProp": "title",
          "regex": {
            "regex": "(?:Chapter\\s+)?(\\d+)",
            "process": "(match)=>match && match.length >= 2 ? parseInt(match[1]) : null"
          }
        },
        {
          "label": "title",
          "element": "a",
          "getContent": true,
          "withHref": false,
          "dataProp": "title"
        },
        {
          "label": "date",
          "getContent": true,
          "withHref": false
        }
      ]
    },
    {
      "label": "CurentPage",
      "element": ".pagination>li.active>a",
      "getContent": true,
      "withHref": false,
      "dataProp": "data-page",
      "transformProcess": "(value)=>parseInt(value) + 1"
    },
    {
      "label": "LastPage",
      "element": ".last>a",
      "getContent": true,
      "withHref": false,
      "dataProp": "data-page",
      "transformProcess": "(value)=>parseInt(value) + 1"
    }
  ]
}