{
  "url": "https://novgo.net${0}",
  "query": [
    {
      "label": "url",
      "element": ".pagination>li.active>a",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": []
    },
    {
      "label": "title",
      "element": ".title",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": [
        0
      ]
    },
    {
      "label": "summary",
      "element": ".desc-text>p",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": []
    },
    {
      "label": "tags",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": []
    },
    {
      "label": "Author",
      "element": ".info>div>a",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": [
        0
      ]
    },
    {
      "label": "Status",
      "element": ".info>div",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": [
        3
      ],
      "regex": {
        "regex": "Status:\\s*([^<]+)",
        "process": "(match)=>match ? match[match.length - 1] : null"
      }
    },
    {
      "label": "Genres",
      "element": ".info>div",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": [
        1
      ],
      "regex": {
        "regex": "^Genre:\\s*",
        "process": "(match)=>match?.input?.replace(match[0], \"\") ?? \"\""
      }
    },
    {
      "label": "Chapters",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": []
    },
    {
      "label": "LastUpdate",
      "getContent": true,
      "withHref": false,
      "selectItemsAtIndex": []
    }
  ]
}